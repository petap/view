<?php
/** @var \Laminas\View\Model\ViewModel $viewModel */
$viewModel = $this->viewModel()->getCurrent();
$trHeadViews = $viewModel->getChildrenByCaptureTo('table-head-rows');
$trBodyViews = $viewModel->getChildrenByCaptureTo('table-body-rows');
?>
<table>
    <thead>
        <?php foreach ($trHeadViews as $view) echo $this->render($view); ?>
    </thead>
    <tbody>
        <?php foreach ($trBodyViews as $view) echo $this->render($view); ?>
    </tbody>
</table>